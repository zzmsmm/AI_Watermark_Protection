t\t\tsaveAsImage: {}\n\t\t\t\t\t}\n\t\t\t\t},\n\t\t\t    grid: {\n\t\t\t        left: '2%',\n\t\t\t        right: '3%',\n\t\t\t        bottom: '3%',\n\t\t\t        containLabel: true\n\t\t\t    },\n\t\t\t    xAxis: {\n\t\t\t        type: 'value',\n\t\t\t        boundaryGap: [0, 0.01]\n\t\t\t    },\n\t\t\t    yAxis: {\n\t\t\t        type: 'category',\n\t\t\t        data: []\n\t\t\t    },\n\t\t\t    series: [\n\t\t\t        {\n\t\t\t            name: '激励总量',\n\t\t\t            type: 'bar',\n\t\t\t            data: [],\n\t\t\t\t\t\titemStyle: {\n                    normal: {\n                        color: (params)=> {\n\t\t\t\t\t\t\tconsole.log(this.my_id)\n\t\t\t\t\t\t\tif(params.dataIndex==this.my_id)\n\t\t\t\t\t\t\treturn '#69d300';\n\t\t\t\t\t\t\telse\n\t\t\t\t\t\t\treturn '#5470C6';\t\t\t\t\t\t                    \n\t\t\t\t\t\t                }\n                    }\n                },\n\t\t\t        }\n\t\t\t    ]\n\t\t\t}\t\t\t\t\n\t\t}\n\t},\n\tcreated(){\n\t},\n\tmounted(){\n\t\tthis.init_echarts()\n\t\tthis.draw_rank()\n\t},\n\twatch: {\t\n\t\toption:{\n\t\t\thandler(){\n\t\t\t\tthis.draw_rank()\n\t\t},\n\t\tdeep:true\n\t\t}\n\n\t},\n\tmethods: {\n\t\tinit_echarts(){\n\t\t\tthis.myChart = echarts.init(document.getElementById('rankchartid'))\n\t\t},\n\t\tdraw_rank(){\n\t\t\tthis.myChart.setOption(this.option,true)\n\t\t},\n\t\t\t\n\t}\n}\n</script>\n\n<style>\n\t.rankchart{\n\t\tmargin-left: 0%;\n\t\tmargin-top: 30px;\n\t}\n</style>"]}]}