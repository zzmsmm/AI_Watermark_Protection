     placeholder=\"输入关键字搜索\"\n\t\t\t  prefix-icon=\"el-icon-search\" />\n\t    </template>\n\t  </el-table-column>\n\t</el-table>\n\t<div v-if=\"!flag\">\n\t\t\t<h2>您无权查看此页面</h2>\n\t\t</div>\n\n</el-card>\n  \n</template>\n\n<script>\nimport {getVoteResult} from '@/api/user'\n  export default {\n\tname: \"nodelist\",\n    data() {\n      return {\n//refresh:\"获取信息\",\n        tableData: [],\n        search: '',\n\t\tflag:true,\n\t\ttimer: null\n      }\n    },\n\tcreated(){\n\t\tthis.getVoteResult()\n\t\tthis.timer = setInterval(() => {\n\t\t\tsetTimeout(this.getVoteResult(), 0)\n\t\t}, 1000 * 5)\n\t},\n    methods: {\n\tgetVoteResult(){\n      getVoteResult().then(response => {\n          const { data } = response\n\t\t  this.flag=response.flag\n          var addr = ''\n          var signature = ''\n          var vote_result = ''\n\t\t  var i=0\n\t\t  console.log(data)\n\t\t  for(var key in data)\n\t\t  {\n\t\t\t  addr='企业 '+String(key)\n\t\t\t//console.log(addr)\n\t\t\t  if(data[key].signature === true)\n          \t\tsignature = '通过'\n          \t  else\n        \t\tsignature = '不通过'\n\n\t\t\t  if(data[key].vote === true)\n          \t\tvote_result = 'positive'\n              else\n         \t\tvote_result = 'negitive'\n\t\t\t\t\n\t\t\tthis.$set(this.tableData,i,{\n            id:String(key),\n            address:addr,\n            mark_check:signature,\n            vote:vote_result\n            }) \n\t\t\ti++        \n          }\n\t\t  })\n\t\t},\n      handleEdit(index, row) {\n        console.log(index, row);\n      },\n      handleDelete(index, row) {\n        console.log(index, row);\n      },\n\t  beforeDestroy() {\n\t  \tclearInterval(this.timer);\n\t  \tthis.timer = null;\n\t  }\n    },\n  }\n</script>"]}]}