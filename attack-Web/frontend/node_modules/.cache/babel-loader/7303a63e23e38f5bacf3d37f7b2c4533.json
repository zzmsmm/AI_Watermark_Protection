\"flag\">\n\t\t  <el-aside width=\"600px\">\n\t\t\t  \n\t\t\t  <el-card >\n\t\t\t  \n\t\t\t\t<h2>\n\t\t\t\t\t<svg-icon icon-class=\"component\" style=\"color: #2a68a7; margin-right: 10px;\"/>子区块信息</h2>\n\t\t\t\t\t<!--\n\t\t\t\t\t<el-button type=\"primary\"\n                   v-text=\"refresh\"\n                   @click=\"getSubBlock()\"\n                   size=\"mini\"\n                   >\n        \t\t\t</el-button>\n\t\t\t\t\t-->\n\t\t\t\t<hr />\n\t\t\t\t\n\t\t\t\t<div style=\"margin-top: 30px;\">\n\t\t\t\t<span><svg-icon icon-class=\"chart\" style=\"color: #2a68a7; margin-right: 10px;\"/>父区块高度</span>\n\t\t\t\t<p><b>{{height}}</b></p> \n\t\t\t\t<span><svg-icon icon-class=\"documentation\" style=\"color: #2a68a7; margin-right: 10px; margin-top: 30px;\"/>父区块哈希</span>\n\t\t\t\t<p><b>{{parenthash}}</b></p>\n\t\t\t\t<span><svg-icon icon-class=\"documentation\" style=\"color: #2a68a7; margin-right: 10px; margin-top: 30px;\"/>子区块哈希</span>\n\t\t\t\t<p><b>{{childhash}}</b></p>\n\t\t\t\t</div>\n\t\t\t  </el-card>\n\t\t  </el-aside>\n\t\t  <el-main>\n\t\t\t  <h2><svg-icon icon-class=\"list\" style=\"color: #2a68a7; margin-right: 10px;\"/>训练者激励分配</h2>\n\t\t\t  <hr />\n\t\t\t  <el-table\n\t\t\t    :data=\"tableData.filter(data => !search || data.id.toLowerCase().includes(search.toLowerCase()))\"\n\t\t\t    height=\"300\" style=\"width: 100%\">\n\t\t\t    <el-table-column\n\t\t\t      label=\"企业名称\"\n\t\t\t      prop=\"id\" align=\"center\">\n\t\t\t    </el-table-column>\n\t\t\t    <el-table-column\n\t\t\t      label=\"激励分配\"\n\t\t\t      prop=\"incentive\" align=\"center\">\n\t\t\t    </el-table-column>\n\t\t\t    <el-table-column\n\t\t\t      align=\"right\">\n\t\t\t      <template slot=\"header\" slot-scope=\"scope\">\n\t\t\t        <el-input\n\t\t\t          v-model=\"search\"\n\t\t\t          size=\"mini\"\n\t\t\t          placeholder=\"输入关键字搜索\"\n\t\t\t  \t\t  prefix-icon=\"el-icon-search\" />\n\t\t\t      </template>\n\t\t\t    </el-table-column>\n\t\t\t  </el-table>\n\t\t  </el-main>\n\t\t  \n\t</el-container>\n</el-card>\n  \n</template>\n\n<script>\nimport {getSubBlock} from '@/api/user'\n  export default {\n\tname: \"blockinfo\",\n    data() {\n      return {\n\t\t  //refresh:\"获取信息\",\n        tableData: [],\n        search: '',\n\t\theight:'',\n\t\tparenthash:'',\n\t\tchildhash:'',\n\t\tflag:true,\n\ttimer: null \n      }\n    },\n\tcreated(){\n\t\tthis.getSubBlock()\n\t\tthis.timer = setInterval(() => {\n\t\t\tsetTimeout(this.getSubBlock(), 0)\n\t\t}, 1000 * 5)\n\t},\n    methods: {\n\t\tgetSubBlock(){\n\t\t\tgetSubBlock().then(response => {\n          const { data } = response\n\t\t  this.height = data.height\n\t\t  this.flag=data.flag\n          this.parenthash = data.parenthash\n          this.childhash = data.childhash\n\t\t  var i =0\n\t\t  for(var key in data.stake)\n\t\t  {\n\t\t\tthis.$set(this.tableData,i,{\n            id:'企业 '+String(key),\n            incentive:data.stake[key]\n            })\n\t\t\ti++\n\t\t  }\n          \n            addr = temp+String(i)\n            \n          \n\t\t\t})\n\t\t},\n      handleEdit(index, row) {\n        console.log(index, row);\n      },\n      handleDelete(index, row) {\n        console.log(index, row);\n      },\n\t\t\tbeforeDestroy() {\n\t\t\t\tclearInterval(this.timer);\n\t\t\t\tthis.timer = null;\n\t\t\t}\n    },\n  }\n</script>"]}]}